                    "fontFamily": "var(--wp--preset--font-family--montserrat)",
                    "fontWeight": "600"
                }
            },
            "h6": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--xx-small)",
						"top": "var(--wp--preset--spacing--x-small)"
					}
				},
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--montserrat)",
                    "fontWeight": "600"
                }
            },
			"heading": {
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--line-height--large)"
